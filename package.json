{
  "name": "cg-2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "node tests",
    "asbuild:untouched": "asc assembly/index.ts -b dist/assets/untouched.wasm -t dist/assets/untouched.wat --sourceMap --debug --runtime none --importMemory",
    "asbuild:optimized": "asc assembly/index.ts -b dist/assets/optimized.wasm -t dist/assets/optimized.wat --optimize  --runtime none --importMemory",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "build": "npm run asbuild",
    "asbuild:watch": "onchange -i 'assembly/**/*' -- npm run asbuild",
    "serve-demo": "static-server",
    "deploy": "npm run asbuild:optimized  && cp index.html dist/ && cp images.js dist/ && cp style.css dist/ && cp assets/test-image.png dist/assets/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrian-goe/cg-2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adrian-goe/cg-2/issues"
  },
  "homepage": "https://adrian-goe.github.io/cg-2",
  "dependencies": {
    "@assemblyscript/loader": "0.17.11"
  },
  "devDependencies": {
    "assemblyscript": "0.17.11",
    "onchange": "7.1.0",
    "static-server": "2.2.1",
    "benchmark": "2.1.4"
  }
}
